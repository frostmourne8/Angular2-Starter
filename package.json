{
  "name": "angular2-starter",
  "version": "0.0.0",
  "description": "An base project to use from future angular2 applications",
  "scripts": {
    "start": "concurrently --kill-others --raw \"npm run start:dev-server\" \"npm run start:dev\"",
    "start:dev-server": "nodemon --exec ts-node --inspect ./server/index.ts",
    "start:prod": "node index.js",
    "start:dev": "webpack-dev-server --config webpack/webpack-dev.js --inline --progress --history-api-fallback",
    "test:ui": "karma start test-configuration/karma.conf.js",
    "test-coverage:ui": "karma start test-configuration/coverage-karma.conf.js",
    "test:server": "ts-node --project server/tsconfig.json node-modules/jasmine/bin/jasmine.js \"server/**/!(*server).ts\"",
    "test-coverage:server": "nyc --report-dir coverage/server npm run test:server",
    "test": "rimraf coverage && npm run test-coverage:ui && npm run test-coverage:server",
    "build": "rimraf dist && webpack --config webpack/webpack-prod.js --progress --profile --bail && npm run build:server",
    "build:server": "npm run compile:server && npm run copy:server",
    "compile:server": "tsc -p server/tsconfig.json --allowJs",
    "copy:server": "copyup server/**/*.json dist/ && copyfiles package.json dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frostmourne8/Angular2-Starter.git"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "inversify": "^4.3.0",
    "jquery": "^3.2.1",
    "nodemon": "^1.12.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "rxjs": "^5.4.3",
    "serve-favicon": "^2.4.5",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.4",
    "@ngtools/webpack": "^1.7.2",
    "@types/body-parser": "^1.16.5",
    "@types/core-js": "0.9.35",
    "@types/express": "^4.0.37",
    "@types/jasmine": "^2.6.0",
    "@types/lodash": "^4.14.76",
    "@types/node": "6.0.46",
    "@types/request": "^2.0.3",
